{
  "name": "nextjs-boilerplate",
  "author": "Matthew Cremer",
  "description": "Nextjs boilerplate with typescript, eslint, and prettier",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "homepage": "https://github.com/mattcremerband",
  "engines": {
    "node": ">=18.0.0",
